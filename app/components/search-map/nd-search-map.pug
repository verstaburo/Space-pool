mixin nd-search-map()
  +b#popup-search-map.nd-search-map__&attributes(attributes)
    +e.wrapper
      +e.BUTTON.close.js-search-map-close Close
      +e.map#search-map
      +e.results
    block

mixin nd-search-map-button()
  +b.BUTTON.js-search-map-show.nd-search-map-button__&attributes(attributes)(type="button") map
    block

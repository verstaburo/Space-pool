mixin header()
  +b.header&attributes(attributes)
    .container.container_flex
      +e.A(href="/").__logo
        +icon('logo').__logo-cube
        +icon('logo2').__logo-text
      +header-menu
      +burger

mixin header-login()
  +b.header&attributes(attributes)
    .container.container_flex
      +e.A(href="/").__logo
        +icon('logo').__logo-cube
        +icon('logo2').__logo-text
      +header-user
      +burger
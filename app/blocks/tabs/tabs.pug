mixin tabs()
  +b.UL.tabs&attributes(attributes)
    block

mixin tab()
  +e.LI.tab&attributes(attributes)
    block

mixin tab-container()
  +e.LI.tab_button&attributes(attributes)
    +e.tab-content
      block

mixin tabs-content()
  .tabs-content&attributes(attributes)
    block

mixin tab-wrapper()
  .tabs-content__wrapper&attributes(attributes)
    block

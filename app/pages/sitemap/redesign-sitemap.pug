extends ../../layouts/redesign-default

block head
  - var pageTitle = 'Spacepool - New Sitemap'

block header
  - var headerClass = ' is-login'
  - var isNoUser = false
  - var isMap = false
  - var host = 'manager'
  - var pageClass = ' nd-page_main'

block content
  - var menuList = [{name: "Общее", href: "#nav-common"}, {name: "Search", href: "#nav-search"}, {name: "Rental Summary", href: "#nav-rental-summary"}, {name: "Deal Passport", href: "#nav-deal"}, {name: "Guest Account", href: "#nav-ga"}, {name: "Host Account", href: "#nav-ha"}, {name: "Edit Space", href: "#nav-edit-space"}, {name: "Add Space", href: "#nav-add-space"}];
  //- , {name: "HA - Single Space", href: "#nav-ha-space"}];
  - var nav = {menu: menuList, menuLinkCls: ' js-nd-anchor'}
  +nd-page-header._grid._grid_acc
    .nd-container.nd-hide-from-md
      +nd-page-header-side
      +nd-page-header-main
        +e.mb-menu.nd-hide-from-md
          +slider-navigation(menuList, 0, '', ' js-nd-anchor')

  +nd-content
    .nd-container
      +nd-content-aside
        +sidebar-nosticky.nd-sidebar.nd-hide-md
          +nd-side-navigation(nav, 'Spaces')._profile

      +nd-content-main
        +b.sitemap
          +e.group#nav-ha-space.nd-hide
            +e.head
              +page-title Страницы Host Account - Single Space (only HD breakpoint)
            +e.list
              +nd-link(href="ha-space.html") Spacepool - Host Space - Dashboard (ha-space.html)
          +e.group#nav-add-space
            +e.head
              +page-title Страницы Add Space
            +e.list
              +nd-link(href="add-space.html") Spacepool - Add Space - Start (add-space.html)
              +nd-link(href="add-space-location.html") Spacepool - Add Space - Location (add-space-location.html)
              +nd-link(href="add-space-description.html") Spacepool - Add Space - Title and Description (add-space-description.html)
              +nd-link(href="add-space-facilities.html") Spacepool - Add Space - Facilities (add-space-facilities.html)
              +nd-link(href="add-space-rules.html") Spacepool - Add Space - House rules(add-space-rules.html)
              +nd-link(href="add-space-photo.html") Spacepool - Add Space - Photo (add-space-photo.html)
              +nd-link(href="add-space-conditions.html") Spacepool - Add Space - Terms and conditions (add-space-conditions.html)
              +nd-link(href="add-space-conditions-2.html") Spacepool - Add Space - Terms and conditions (page 2) (add-space-conditions-2.html)
              +nd-link(href="add-space-listing.html") Spacepool - Add Space - Listing (add-space-listing.html)
          +e.group#nav-edit-space
            +e.head
              +page-title Страницы Host Account - Edit Space
            +e.list
              +nd-link(href="ha-edit-space.html") Spacepool - Edit Space - Location (ha-edit-space.html)
              +nd-link(href="ha-edit-space-description.html") Spacepool - Edit Space - Title and Description (ha-edit-space-description.html)
              +nd-link(href="ha-edit-space-facilities.html") Spacepool - Edit Space - Facilities (ha-edit-space-facilities.html)
              +nd-link(href="ha-edit-space-rules.html") Spacepool - Edit Space - House Rules (ha-edit-space-rules.html)
              +nd-link(href="ha-edit-space-photos.html") Spacepool - Edit Space - Photos (ha-edit-space-photos.html)
              +nd-link(href="ha-edit-space-conditions.html") Spacepool - Edit Space - Terms and conditions (ha-edit-space-conditions.html)
              +nd-link(href="ha-edit-space-conditions-2.html") Spacepool - Edit Space - Terms and conditions (page 2) (ha-edit-space-conditions-2.html)
          +e.group#nav-ha
            +e.head
              +page-title Страницы Host Account (only HD breakpoint)
            +e.list
              +nd-link(href="host-account.html") Spacepool - Host Account - Spaces (host-account.html)
              +nd-link(href="host-account-messages.html") Spacepool - Host Account - Messages (host-account-messages.html)
